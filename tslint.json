{
  "extends": "tslint:recommended",
  "rulesDirectory": [],
  "rules": {
    "max-line-length": {
      "options": [
        170
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        170
      ]
    }
  }
}
